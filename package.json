{
    "name": "notex",
    "description": "editor",
    "license": "GPL-3.0",
    "dependencies": {
        "ipfs": "^0.40.0",
        "markdown-it": "^10.0.0",
        "markdown-it-abbr": "^1.0.4",
        "markdown-it-emoji": "^1.4.0",
        "markdown-it-footnote": "^3.0.2",
        "markdown-it-mark": "^3.0.0",
        "markdown-it-math": "^4.1.1",
        "snabbdom": "^0.7.4"
    },
    "devDependencies": {
        "@types/bootstrap": "^4.3.1",
        "@types/codemirror": "^0.0.82",
        "@types/iscroll": "^5.2.3",
        "@types/jquery": "^3.3.31",
        "@types/mathjax": "^0.0.36",
        "@types/node": "^13.1.0",
        "sass": "^1.24.0",
        "terser": "^4.4.3",
        "typescript": "^3.7.4"
    },
    "repository": {
        "url": "git@bitbucket.org:hsk81/notex.git"
    },
    "scripts": {
        "prebuild": "npx tsc -b && npx sass ./static/css --no-source-map",
        "build": "./notex/minify.py",
        "predebug": "npx tsc -b && npx sass ./static/css --no-source-map",
        "debug": "DEBUG=1 gunicorn -c config.py wsgi:app",
        "prestart": "npm run build",
        "start": "DEBUG=0 gunicorn -c config.py wsgi:app --preload",
        "watch": "npx tsc -w & npx sass ./static/css --no-source-map --watch"
    },
    "version": "3.2.0"
}
