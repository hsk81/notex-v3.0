{
    "name": "notex",
    "description": "editor",
    "license": "GPL-3.0",
    "dependencies": {
        "ipfs": "^0.40.0",
        "markdown-it": "^10.0.0",
        "markdown-it-abbr": "^1.0.4",
        "markdown-it-anchor": "^5.2.5",
        "markdown-it-decorate": "^1.2.2",
        "markdown-it-emoji": "^1.4.0",
        "markdown-it-footnote": "^3.0.2",
        "markdown-it-mark": "^3.0.0",
        "markdown-it-math": "^4.1.1",
        "markdown-it-video": "^0.6.3"
    },
    "devDependencies": {
        "@types/bootstrap": "^4.3.1",
        "@types/codemirror": "^0.0.82",
        "@types/iscroll": "^5.2.3",
        "@types/jquery": "^3.3.33",
        "@types/node": "^13.7.7",
        "browserify": "^16.5.0",
        "sass": "^1.26.2",
        "terser": "^4.6.4",
        "typescript": "^3.8.3"
    },
    "repository": {
        "url": "git@bitbucket.org:hsk81/notex.git"
    },
    "scripts": {
        "prebuild": "./prepack.sh && npx tsc -b && npx sass ./static/css --no-source-map",
        "build": "./notex/minify.py",
        "predebug": "./prepack.sh && npx tsc -b && npx sass ./static/css --no-source-map",
        "debug": "source ./bin/activate && DEBUG=1 gunicorn -c config.py wsgi:app",
        "prestart": "source ./bin/activate && npm run build",
        "start": "source ./bin/activate && DEBUG=0 gunicorn -c config.py wsgi:app --preload",
        "watch": "npx tsc -w & npx sass ./static/css --no-source-map --watch"
    },
    "engines": {
        "node": "^13.5.0"
    },
    "version": "3.4.8"
}
